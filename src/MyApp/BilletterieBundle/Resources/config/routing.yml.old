my_app_billetterie_home:
    path:      /
    defaults:
        _controller: MyAppBilletterieBundle:Default:index

my_app_billetterie_form:
    path:      /form/{etape}
    defaults:
        _controller: MyAppBilletterieBundle:Default:form
        etape:        1    
    requirements:
        etape: \d*

my_app_billetterie_tarif:
    path:      /tarif
    defaults:
        _controller: MyAppBilletterieBundle:Default:tarif

my_app_billetterie_mention:
    path:      /mention
    defaults:
        _controller: MyAppBilletterieBundle:Default:mention
